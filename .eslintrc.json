{
  "plugins": ["jest", "@typescript-eslint"],
  "extends": ["plugin:github/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "./tsconfig.eslint.json"
  },
  "rules": {
    "i18n-text/no-en": 0
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}
